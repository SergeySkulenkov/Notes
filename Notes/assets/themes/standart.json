{
    "name":"Standart",
    "windowTitle":{
        "icon": "assets/themes/standart/ico.png",
        "background-color":"#333"
    },
    "mainMenu":{
        "color":"#ffffff",
        "item-selected-background-color":"#656565",
        "item-pressed-background-color":"#6f6f6f",
        "item-padding":"5px"
    },
    "windowButtons":{
        "minimaze-button":{
            "normal":{
                "qproperty-icon":"url(assets/themes/standart/min-btn.png)",
                "border":"none"
            },
            "hover":{
                "background":"#454545"
            },
            "pressed":{
                "background":"#373737"
            }
        },
        "maximaze-button":{
            "normal":{
                "qproperty-icon":"url(assets/themes/standart/max-btn.png)",
                "border":"none"
            },
            "hover":{
                "background":"#454545"
            },
            "pressed":{
                "background":"#373737"
            }
        },
        "normal-button":{
            "normal":{
                "qproperty-icon":"url(assets/themes/standart/normal-btn.png)",
                "border":"none"
            },
            "hover":{
                "background":"#454545"
            },
            "pressed":{
                "background":"#373737"
            }
        },
        "close-button":{
            "normal":{
                "background":"url(assets/themes/standart/close-btn.png) no-repeat center center",
                "border":"none"
            },
            "hover":{
                "background":"#ee3520 url(assets/themes/standart/close-btn-hover.png) no-repeat center center"
            },
            "pressed":{
                "background":"#ee3520 url(assets/themes/standart/close-btn-hover.png) no-repeat center center"
            }
        }
    },
    "leftColumn":{
        "background":"#2d2d2d",
        "add-button":{
            "normal":{
                "qproperty-icon":"url(assets/themes/standart/plus-left.png)",
                "border":"none",
                "background":"#444444",
                "border-top-left-radius":"3px",
                "border-bottom-left-radius":"3px"
            },
            "hover":{
                "background":"#555555"
            },
            "pressed":{
                "background":"#474747"
            }
        },
        "add-combobox":{
            "border":"none",
            "border-left":"1px solid #555555",
            "background":"#444444",
            "border-top-right-radius":"3px",
            "border-bottom-right-radius":"3px",
            "color":"#cccccc",
            "font-size":"12px",
            "padding-left":"10px"
        },
        "add-combobox-drop-down":{
            "border-top-right-radius":"3px",
            "border-bottom-right-radius":"3px",
            "width":"3px"
        },
        "add-combobox-item-view":{
            "background":"#ffffff"
        },
        "add-combobox-item":{
            "padding":"5px"
        },
        "toolbox-page":{
            "background":"#3a3a3a"
        },
        "toolbox-tab":{
            "background":"#2d2d2d",
            "color":"#eeeeee",
            "font-size":"14px",
            "border":"none"
        },
        "toolbox-tab-selected":{
            "background":"#474747",
            "border-radius":"3px",
            "color":"#eeeeee"

        },
        "toolbox-tab-height": 30,
        "toolbox-tab-notepads-icon": "assets/themes/standart/notepad_ite.png",
        "toolbox-tab-tasks-icon": "assets/themes/standart/check_item.png",
        "toolbox-tab-notes-icon": "assets/themes/standart/notepad_ite.png",
        "tree-widget":{

        }

    },
    "editor":{
        "title":{
            "font-size": "16px",
            "color":"#828282",
            "border":"2px solid #fff",
            "background": "#fff",
            "margin":"5px"
        },
        "title-focus":{
            "border-bottom-color": "#86d4fc",
            "color":"#555"
        },
        "tags":{
            "font-size": "11px",
            "color":"#777",
            "border":"none",
            "margin-bottom":"5px"
        },
        "buttons-bar":{
            "background":"#eee",
            "border":"1px solid #eee",
            "border-radius":"3px",
            "margin":"5px",
            "min-height":"34px"
        },
        "buttons":{
            "normal-button":{
                 "background":"#eee",
                 "border": "1px solid #eee",
                 "border-radius": "3px",
                 "min-height":"24px",
                 "min-width": "24px"
            },
            "hover-button":{
                "background":"#ddd",
                "border-color": "#aaa"
            },
            "save-button-icon":"assets/themes/standart/floppy-disk.svg",
            "bold-button-icon":"assets/themes/standart/bold.svg",
            "italic-button-icon":"assets/themes/standart/italic.svg",
            "underline-button-icon":"assets/themes/standart/underline.svg",
            "strikethrough-button-icon":"assets/themes/standart/strikethrough.svg",
            "tags-icon":"assets/themes/standart/tags.png",
            "prev-button-icon":"assets/themes/standart/prev_item.png",
            "next-button-icon":"assets/themes/standart/next_item.png"
        },
        "prev-button":{
            "border":"1px solid #ccc",
            "background": "#eee"
        }

    }
}
